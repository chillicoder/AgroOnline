<h1>Listing Books</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Last Name</th>
    <th>Nickname</th>
    <th>Country</th>
  </tr>

  <% @providerList.each do | provider | %>
    <tr>
      <td><%= provider.id %></td>
      <td><%= provider.name %></td>
      <td><%= provider.lastName %></td>
      <td><%= provider.nickname %></td>
      <td><%= provider.country %></td>
    </tr>
  <% end %>
</table>

<br>
